<script type="text/javascript">
  $(document).ready(function(){
    $("#callback_submit").click(function(){
      
      $("#callback_form").validate();
      
      if ($('#callback_form').valid())
      {
        $("#callback_submit").prop('disabled', true);
        if($('textarea[id="g-recaptcha-response"]').val()=="")
        {
            $("#captcha_error").html("Captcha Required");
        }
        else
        {
            $.ajax({
                type:"POST",
                url: 'callback_request.php',
                dataType: 'json',
                data: {
                       "g-recaptcha-response":$('textarea[id="g-recaptcha-response"]').val(),
                       "name":$("#callback_name").val(),
                       "phone":$("#callback_phone").val(),
                       "email":$("#callback_email").val(),
                       "course":$("#callback_course").val(),
                       "datetime":$("#callback_datetime").val(),
                       "message":$("#callback_message").val(),
                       },

                success: function( response ){
                    $("#callback_submit").prop('disabled', false);
                    $("#chat_entry").hide();
                    $("#callback_success").fadeIn();                    
                    $(".chat_container").delay(2000).slideToggle("hide");

                },
                error: function( error ){
                    $("#callback_submit").prop('disabled', false);
                    $("#chat_entry").hide();
                    $("#callback_success").fadeIn();                    
                    $(".chat_container").delay(4000).slideToggle("hide");
                }
            });
        }
      }
    });
  });
</script>

